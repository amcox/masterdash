<h1>Listing vams</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Teaching</th>
      <th>Percentile</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @vams.each do |vam| %>
      <tr>
        <td><%= vam.subject %></td>
        <td><%= vam.teaching %></td>
        <td><%= vam.percentile %></td>
        <td><%= link_to 'Show', vam %></td>
        <td><%= link_to 'Edit', edit_vam_path(vam) %></td>
        <td><%= link_to 'Destroy', vam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vam', new_vam_path %>
